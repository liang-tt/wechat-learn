/* pages/classic/classic.wxss */
.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
}
.header {
  width: 690rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding: 30rpx 30rpx 0 30rpx;
  border-top: 1px solid#f5f5f5;
  border-bottom: 1px solid#f5f5f5;
}

.left-container {
  display: flex;
  flex-direction: row;
  line-height: 60rpx;
}

.navi {
  position:absolute;
  bottom:40rpx;
}

.btn-share {
  margin-left: 35rpx;
}

.icon-share {
  width: 40rpx;
  height: 40rpx;
}

